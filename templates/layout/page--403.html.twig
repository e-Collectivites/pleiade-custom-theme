  color: white;
}
svg {
  width: 50vw;
  font-family: "Bungee", cursive;
}
.lightblue {
  fill: #1f3889;
}
.eye {
  cx: calc(115px + 30px * var(--mouse-x));
  cy: calc(50px + 30px * var(--mouse-y));
}
#eye-wrap {
  overflow: hidden;
}
