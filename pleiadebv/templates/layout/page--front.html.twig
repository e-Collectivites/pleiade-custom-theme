                </div>
              </div>
            </div>
            <div id="zimbra_block_id">
              <div class="d-flex justify-content-center">
                <div id="spinner-div-zimbra" class="spinner-border spinner-border-sm text-primary" role="status">
                </div>
              </div>
            </div>
          </div>
          
          <!-- ============================================================== -->
          <!-- End Recréation de blocks Lemon classiques géré par le module pleiade  -->
          <!-- ============================================================== -->
            
 
          <!-- ============================================================== -->
          <!-- Blocks de démo / test -->
          <!-- ============================================================== -->
            {% include '@pleiadebv/templates/layout/parts/demo/blocs-demo.html.twig' %}
          <!-- ============================================================== -->
